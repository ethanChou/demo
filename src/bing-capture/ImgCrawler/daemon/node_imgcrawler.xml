<service>
	<id>node_imgcrawler.exe</id>
	<name>node_imgcrawler</name>
	<description>图片爬虫服务(Bing)</description>
	<executable>C:\Program Files\nodejs\node.exe</executable>
	<logmode>rotate</logmode>
	<argument>--harmony</argument>
	<argument>D:\WorkSpace\node-js-demo\bing-capture\ImgCrawler\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>D:\WorkSpace\node-js-demo\bing-capture\ImgCrawler\index.js</argument>
	<argument>--log</argument>
	<argument>node_imgcrawler wrapper</argument>
	<argument>--grow</argument>
	<argument>0.25</argument>
	<argument>--wait</argument>
	<argument>1</argument>
	<argument>--maxrestarts</argument>
	<argument>3</argument>
	<argument>--abortonerror</argument>
	<argument>n</argument>
	<argument>--stopparentfirst</argument>
	<argument>undefined</argument>
	<stoptimeout>30sec</stoptimeout>
	<workingdirectory>D:\WorkSpace\node-js-demo\bing-capture\ImgCrawler</workingdirectory>
</service>